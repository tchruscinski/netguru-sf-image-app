    <enableStreamingApi>true</enableStreamingApi>
    <externalSharingModel>Private</externalSharingModel>
    <fields>
        <fullName>AlbumId__c</fullName>
        <description>Image Id from the Image API</description>
        <externalId>false</externalId>
        <label>AlbumId</label>
        <length>10</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>DisplayedImage__c</fullName>
        <description>Formula displaying image from the url stored on the object</description>
        <externalId>false</externalId>
        <formula>IMAGE(ImageUrl__c, &quot;Image&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Displayed Image</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ImageApiId__c</fullName>
        <caseSensitive>false</caseSensitive>
        <description>Id from the Image API</description>
        <externalId>true</externalId>
        <label>ImageApiId</label>
        <length>10</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>ImageTitle__c</fullName>
        <description>Image title from the Image API</description>
        <externalId>false</externalId>
        <label>Image Title</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ImageUrl__c</fullName>
        <description>Image Url from the Image API, used as External Id</description>
        <externalId>false</externalId>
        <label>Image Url</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ThumbnailUrl__c</fullName>
        <description>Thumbnail url from the Image API</description>
        <externalId>false</externalId>
        <label>Thumbnail Url</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Image</label>
    <listViews>
        <fullName>All</fullName>
        <columns>NAME</columns>
        <columns>ImageUrl__c</columns>
        <columns>CREATED_DATE</columns>
        <filterScope>Everything</filterScope>
